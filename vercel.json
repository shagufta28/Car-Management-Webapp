{
    "version": 2,
    "builds": [
      { "src": "/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { 
        "src": "/api/(.*)",
       "dest": "/"
     },
     {
        "src": "/cars",
       "dest": "/"
     },
     {
        "src": "/auth",
       "dest": "/"
     },
     {
        "src": "/auth/admin/login",
       "dest": "/"
     },
     {
        "src": "/auth/user/login",
       "dest": "/"
     }
    ]
  }
  