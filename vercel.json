{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/cars",
        "dest": "/index.js"
      },
      {
        "src": "/auth",
        "dest": "/index.js"
      },
      {
        "src": "/auth/admin/login",
        "dest": "/admin/login"
      },
      {
        "src": "/auth/user/login",
        "dest": "/auth"
      }
    ]
  }
  